{
  "name": "vui-lib",
  "version": "0.1.3",
  "description": "A Library For Vu2.0(just learn)",
  "main": "dist/vui.js",
  "style": "dist/index.css",
  "author": "yinggang.qu@gmail.com",
  "files": [
    "dist",
    "src",
    "packages"
  ],
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "deploy:build": "rimraf examples/homepage && node build/deploy-build.js && gh-pages -d examples/homepage",
    "lint": "eslint --ext .js,.vue src",
    "clean": "rimraf dist",
    "changelog": "conventional-changelog -p angular -r 2 -i CHANGELOG.md -s",
    "build:components": "node build/build-components.js",
    "build:entry": "npm run build:components && node build/build-entry.js",
    "build:style": "gulp build --gulpfile build/build-style.js",
    "build": "npm run clean && npm run build:entry && npm run lint && npm run build:style && node build/build.js",
    "pub": "sh build/release.sh",
    "commit": "git-cz"
  },
  "homepage": "https://quyinggang.github.io/vui/",
  "repository": "git@github.com:quyinggang/vui.git",
  "license": "MIT",
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node build/verify-commit-msg.js"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.vue": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "vue": "^2.5.2"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.2.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "chalk": "^2.0.1",
    "commitizen": "^3.0.7",
    "conventional-changelog-cli": "1.3.22",
    "css-loader": "^0.28.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.0.0",
    "file-loader": "^1.1.4",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "gh-pages": "^2.0.1",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-cssmin": "^0.2.0",
    "gulp-sass": "^4.0.2",
    "html-webpack-plugin": "^2.30.1",
    "lint-staged": "^8.1.5",
    "progress-bar-webpack-plugin": "^1.12.1",
    "rimraf": "^2.6.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^1.1.1",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^1.1.2",
    "vue-loader": "^13.3.0",
    "vue-router": "^3.0.6",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^3.1.11",
    "webpack-merge": "^4.1.0",
    "yorkie": "^2.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
